  "dependencies": {
    "bootstrap-sass": "=3.3.7",
    "font-awesome": "=4.6.3",
    "jquery": "=2.1.4"
  },
  "devDependencies": {
    "autoprefixer": "=6.3.7",
    "babel-preset-es2015": "=6.9.0",
    "babelify": "=7.3.0",
    "browserify": "=13.1.0",
    "clean-css": "=3.4.19",
    "eslint": "=1.5.0",
    "eslint-config": "git://github.com/dabapps/eslint-config.git",
